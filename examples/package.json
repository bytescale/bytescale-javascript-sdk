{
  "name": "@upload.io/upload-js-examples",
  "version": "0.0.0",
  "author": "Upload.io <hello@upload.io> (https://upload.io)",
  "private": true,
  "scripts": {
    "start": "npm run start:dev",
    "start:dev": "webpack serve --config webpack.config.dev.js",
    "start:dist": "webpack serve",
    "test": "npm run test:local",
    "preinstall": "npm cache clean --force && (cd ../lib && npm pack)",
    "install": "(rm -rf ./node_modules/upload-js && tar zxvf ../lib/upload-js.tgz -C . && mv package ./node_modules/upload-js) && webdriver-manager update --versions.chrome 85.0.4183.121",
    "test:local": "echo \"Please ensure server has been started with 'npm start'...\" && protractor tests/config.js",
    "test:remote": "protractor tests/config.js $1"
  },
  "dependencies": {
    "regenerator-runtime": "^0.13.7",
    "upload-js": "../lib/upload-js.tgz"
  },
  "devDependencies": {
    "@babel/cli": "7.11.6",
    "@babel/core": "7.11.6",
    "@babel/preset-env": "7.11.5",
    "@typescript-eslint/eslint-plugin": "4.8.1",
    "@typescript-eslint/parser": "4.8.1",
    "babel-loader": "8.2.1",
    "eslint": "7.13.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-config-standard-with-typescript": "19.0.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.1.0",
    "html-webpack-plugin": "4.5.0",
    "jasmine-reporters": "2.3.2",
    "protractor": "7.0.0",
    "protractor-jasmine2-screenshot-reporter": "0.5.0",
    "ts-loader": "8.0.11",
    "typescript": "4.1.2",
    "webpack": "5.5.1",
    "webpack-cli": "4.3.0",
    "webpack-dev-server": "3.11.0"
  }
}
